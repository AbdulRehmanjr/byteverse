generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}
model User {
    userId String @id @default(cuid())
    name String 
}
model Task {
    taskId String @id @default(cuid())
    title String @default("none")
    description String @default("none")
}